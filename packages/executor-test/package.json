  "devDependencies": {
    "@tpmjs/test": "workspace:*",
    "@tpmjs/tsconfig": "workspace:*",
    "@types/node": "^22.15.29",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
